<h1>Editing folder</h1>

<%= render 'form' %>



<br>List all subjects:</br>
   <table>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>Credits</th>      
    </tr>

  
  <% for @subject in Subject.all %>
    <tr>

      <td><%= @subject.code %></td>
      <td><%= @subject.name %></td>
      <td><%= @subject.credits %></td>
    </tr>
  <% end %>
  </table>

<%= link_to 'Show', @folder %> |
<%= link_to 'Back', folders_path %>
